{
    "version": 2,
    "rewrites": [
        { 
            "source": "/dir/(.*)",
            "destination": "/dir/index"
        }
    ],
    "builds": [
        {
            "src": "dir/main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/dir/(.*)",
            "dest": "/dir/main.py"
        }
    ]
}